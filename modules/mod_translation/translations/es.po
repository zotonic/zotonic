msgstr "Editar"

#: modules/mod_translation/templates/admin_translation.tpl:78
#: modules/mod_translation/templates/admin_translation.tpl:85
msgid "Edit language"
msgstr "Editar idioma"

#: modules/mod_translation/templates/admin_translation.tpl:46
msgid "Enabled"
msgstr "Activado"

#: modules/mod_translation/templates/admin_translation.tpl:34
msgid "Enabled languages show up in the language selection menu. The default language is used for new visitors without a selected language."
msgstr "Los idiomas activos aparecen en el menú de selección de idiomas. El idioma por defecto se usa para nuevos visitantes que no hayan seleccionado un idioma."

#: modules/mod_translation/templates/_dialog_language_edit.tpl:8
msgid "Enter the iso code (for example <em>en</em> or <em>nl</em>), and the title of the language. Use the native form of the language, for example <em>English</em>, <em>Türkçe</em> or <em>Français</em>."
msgstr "Ingrese el código ISO (por ejemplo <em>en</em> or <em>es</em>), y el título del idioma. Use el nombre del idioma en su forma nativa, por ejemplo <em>English</em>, <em>Türkçe</em> o <em>Français</em>."

#: modules/mod_translation/templates/admin_translation.tpl:13
msgid "Generate .pot files"
msgstr "Generar archivos .pot"

#: modules/mod_translation/templates/_admin_edit_sidebar.tpl:13
msgid "Help about translations."
msgstr "Ayuda acerca de las traducciones."

#: modules/mod_translation/templates/admin_translation.tpl:48
