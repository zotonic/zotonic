#: modules/mod_oembed/templates/_media_upload_panel.tpl:64
msgid "Cancel"
msgstr "İptal"

#: modules/mod_oembed/templates/_media_upload_panel.tpl:15
msgid "Embed URL"
msgstr "Yerleştirme adresi"

#: modules/mod_oembed/templates/_media_upload_panel.tpl:5
msgid "Embed a video or other media URL. Here you can paste any URL from YouTube, Vimeo or other services."
msgstr "Buraya youtube, vimeo veya diğer servislerin adreslerini yapıştırabilirsiniz."

#: modules/mod_oembed/mod_oembed.erl:238
msgid "Invalid or unsupported media URL. The item might have been deleted or is not public."
msgstr "Geçersiz veya desteklenmeyen medya adresi. Medya silinmiş veya herkese açık olmayabilir."

#: modules/mod_oembed/mod_oembed.erl:204
msgid "Made the media page."
msgstr "Medya sayfası oluştur."

#: modules/mod_oembed/templates/_media_upload_panel.tpl:65
msgid "Make media item"
msgstr "Medya öğesi oluştur"

#: modules/mod_oembed/templates/_media_upload_panel.tpl:48
msgid "Media title"
msgstr "Medya başlığı"

#: modules/mod_oembed/templates/_media_upload_panel.tpl:65
msgid "Replace media item"
msgstr "Medya öğesini değiştir"

#: modules/mod_oembed/templates/_media_upload_panel.tpl:56
msgid "Summary"
msgstr "Özet"

#: modules/mod_oembed/templates/_media_upload_panel.tpl:36
msgid "The media title will be automatically detected from its URL."
msgstr "Medya başlığı bu adresten otomatik olarak alınacaktır."

#: modules/mod_oembed/templates/_media_upload_panel.tpl:19
msgid "Try URL"
msgstr "Adresi dene"

