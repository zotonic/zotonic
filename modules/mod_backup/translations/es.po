#: modules/mod_backup/templates/admin_backup.tpl:27
msgid "Actions"
msgstr "Acciones"

#: modules/mod_backup/templates/admin_backup.tpl:3
msgid "Admin Backups"
msgstr "Copias de Resguardo del Administrador"

#: modules/mod_backup/templates/admin_backup.tpl:12
msgid "At any moment you can make a backup of your system."
msgstr "En cualquier momento puede hacer una copia de resguardo de sus sistema."

#: modules/mod_backup/templates/admin_backup.tpl:10
#: modules/mod_backup/templates/admin_backup.tpl:23
#: modules/mod_backup/templates/_admin_menu_module.tpl:2
msgid "Backups"
msgstr "Copias de Resguardo"

#: modules/mod_backup/templates/admin_backup.tpl:26
msgid "Date"
msgstr "Fecha"

#: modules/mod_backup/templates/admin_backup.tpl:48
msgid "No backups present."
msgstr "No hay copias de resguardo presentes."

#: modules/mod_backup/templates/admin_backup.tpl:18
msgid "Start backup now"
msgstr "Comenzar copia de resguardo ahora"

#: modules/mod_backup/templates/admin_backup.tpl:12
msgid "The backup comprises two parts, the database and the uploaded files."
msgstr "La copia de resguardo consta de dos partes, la base de datos y los archivos subidos."

#: modules/mod_backup/templates/admin_backup.tpl:13
msgid "You can have 10 backups, older ones will be deleted automatically."
msgstr "Puede tener hasta 10 copias de resguardo, las más viejas serán eliminadas automáticamente"

#: modules/mod_backup/templates/admin_backup.tpl:17
msgid "You can start a backup immediately, whilst the backup is running you can continue working."
msgstr "Puede comenzar la copia de resguardo de inmediato, mientras que esté corriendo puede continuar trabajando."

#: modules/mod_backup/templates/admin_backup.tpl:40
msgid "download database"
msgstr "bajar base de datos"

#: modules/mod_backup/templates/admin_backup.tpl:41
msgid "download files"
msgstr "bajar archivos"

#: modules/mod_backup/templates/admin_backup.tpl:38
msgid "this backup is in progress"
msgstr "esta copia de resguardo está en progreso"

